{
    "$schema": "https://playground.wordpress.net/blueprint-schema.json",
    "preferredVersions": {
        "php": "8.2",
        "wp": "beta"
    },
    "features": {
        "networking": true
    },
    "plugins": [
        "gutenberg"
    ],
    "siteOptions": {
        "blogname": "Guten-Demo"
    },
    "landingPage": "/wp-admin/post.php?post=4&action=edit",
    "steps": [
        {
            "step": "login",
            "username": "admin",
            "password": "password"
        },
        {
            "step": "installPlugin",
            "pluginData": {
                "resource": "wordpress.org/plugins",
                "slug": "interactive-code-block"
            }
        },
        {
            "step": "installTheme",
            "themeData": {
                "resource": "wordpress.org/themes",
                "slug": "twentytwentyfour"
            },
            "options": {
                "activate": false
            }
        }
    ]
}